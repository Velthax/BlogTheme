      }
      20% {
        transform: translate(-2px, 2px);
        text-shadow: 0 0 12px rgba(0, 255, 255, 0.8), 2px -2px 0px var(--color-accent-magenta);
      }
      40% {
        transform: translate(-2px, -2px);
        text-shadow: 0 0 8px rgba(0, 255, 255, 0.6), -2px 2px 0px var(--color-accent-green);
      }
      60% {
        transform: translate(2px, 2px);
        text-shadow: 0 0 15px rgba(0, 255, 255, 0.9), 2px 2px 0px var(--color-accent-blue);
      }
      80% {
        transform: translate(2px, -2px);
        text-shadow: 0 0 10px rgba(0, 255, 255, 0.7), -2px -2px 0px var(--color-accent-orange);
      }
      100% {
        transform: translate(0);
        text-shadow: 0 0 10px rgba(0, 255, 255, 0.7);
      }
    }

    /* Scanline as a filling progress bar */
    .scanline {
      width: 0%; /* Start at 0 */
      height: 6px; /* Thicker */
      background: linear-gradient(to right, var(--color-accent-magenta), var(--color-accent-blue), var(--color-accent-green)); /* Gradient fill */
      margin-top: 2rem;
      border-radius: 3px;
      box-shadow: 0 0 25px var(--color-accent-magenta), 0 0 10px var(--color-accent-magenta) inset;
      animation: progress-fill var(--total-intro-duration) ease-out forwards; /* New animation for filling */
      max-width: 80%; /* Limit width */
    }

    @keyframes progress-fill {
      0% { width: 0%; opacity: 0.8; }
      90% { width: 90%; opacity: 1; } /* Fill almost completely */
      100% { width: 100%; opacity: 0; } /* Then fade out */
    }

    /* New: Background lines for intro screen */
    .intro-background-lines {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-image: linear-gradient(45deg, rgba(0, 255, 255, 0.05) 25%, transparent 25%, transparent 75%, rgba(0, 255, 255, 0.05) 75%),
                        linear-gradient(-45deg, rgba(255, 0, 255, 0.05) 25%, transparent 25%, transparent 75%, rgba(255, 0, 255, 0.05) 75%);
      background-size: 80px 80px;
      animation: background-lines-move 20s linear infinite;
      z-index: -1;
      opacity: 0.3;
    }

    @keyframes background-lines-move {
      0% { background-position: 0 0; }
      100% { background-position: 80px 80px; }
    }

    body:not(.intro-finished) > *:not(#intro-screen) {
      opacity: 0;
      visibility: hidden;
    }
    body.intro-finished > *:not(#intro-screen) {
      opacity: 1;
      visibility: visible;
    }
    
  ]]></b:skin>
</head>
